{
    "name": "wenbinye/tars-call",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Ye Wenbin",
            "email": "wenbinye@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "kuiper/tars": "^0.7",
        "pear/net_dns2": "^1.5",
        "symfony/filesystem": "^5.3",
        "wenbinye/tars-gen": "^0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "wenbinye\\tars\\call\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "wenbinye\\tars\\call\\": "tests/"
        }
    },
    "scripts": {
        "gen": "./vendor/bin/tars-gen"
    },
    "extra": {
        "kuiper": {
            "config-file": "src/container.php"
        }
    }
}
